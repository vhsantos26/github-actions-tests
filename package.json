{
  "name": "aljazeera-mobile",
  "version": "2.4.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-release": "react-native run-android --variant=release",
    "clean-android": "cd android && ./gradlew clean",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start-clean": "react-native start --reset-cache",
    "build-querymap": "npx persistgraphql ./src/graphQL/queries alpha_query_map.json --add_typename",
    "contract-tests": "jest -c=jest.config.contract.js",
    "test": "jest --passWithNoTests",
    "test:e2e-ios": "yarn run test:clean && npx codeceptjs run -c codecept.conf.ios.js --verbose -p allure",
    "test:remote-e2e": "npx codeceptjs run -c codecept.conf.saucelabs.js --verbose -p allure",
    "test:e2e-android": "yarn run test:clean && npx codeceptjs run -c codecept.conf.android.js --verbose -p allure",
    "test:clean": "rimraf __tests__/e2e/output",
    "unit-tests": "jest __tests__/unit --coverage",
    "local-unit-tests": "jest",
    "unit-tests-debug": "node --inspect-brk --inspect ./node_modules/.bin/jest -i",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "post-audit": "node scripts/security-audit.js",
    "prepare": "husky install"
  },
  "dependencies": {
